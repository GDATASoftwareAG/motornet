{
  "RabbitMQConsumer": {
    "Host": "localhost",
    "VirtualHost": "/",
    "User": "guest",
    "Password": "guest",
    "Queue": {
      "Name": "someTestQ",
      "MaxLength": 1000000,
      "Bindings": [
        {
          "Exchange": "amq.topic",
          "RoutingKey": "serviceQueue"
        }
      ]
    }
  },
  "RabbitMQPublisher": {
    "Host": "localhost",
    "VirtualHost": "/",
    "User": "guest",
    "Password": "guest",
    "PublishingTarget": {
      "Exchange": "amq.topic",
      "RoutingKey": "destinationRoutingKey"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information"
    }
  },
  "Tracing": {
    "AgentHost": "localhost"
  },
  "Prometheus": {
    "Port": 9110
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "TestConfig": "appsettings.json"
}