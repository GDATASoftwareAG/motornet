{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "RabbitMQConsumer": {
    "Host": "localhost",
    "VirtualHost": "/",
    "User": "guest",
    "Password": "guest",
    "Queue": {
      "Name": "ExampleQueue",
      "Bindings": [
        {
          "Exchange": "amq.topic",
          "RoutingKey": "input"
        }
      ],
      "DeadLetterExchange": {
        "Binding": {
          "RepublishInvalidInputToDeadLetterExchange": false,
          "Exchange": "amq.topic",
          "RoutingKey": "dlx.input"
        }
      }
    },
    "PrefetchCount": 1,
    "Tls": {
      "Enabled": true
    }
  }
}