{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "KafkaConsumer": {
    "GroupId": "group-id",
    "BootstrapServers": "localhost:9092",
    "EnableAutoCommit": false,
    "Topic": "my.input",
    "StatisticsIntervalMs": 5000
  },
  "KafkaPublisher": {
    "ClientId": "client-id",
    "BootstrapServers": "localhost:9092",
    "Topic": "my.output"
  }
}