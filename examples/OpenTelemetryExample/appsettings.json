{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "System": "Warning"
            }
        }
    },
    "JaegerExporter": {
        "AgentHost": "agent-host.localhost"
    },
    "OpenTelemetry": {
        "SamplingProbability": 1,
        "Sources": ["OpenTelemetryExample", "MotorNet.OpenTelemetry"]
    },
    "RabbitMQConsumer": {
        "Host": "localhost",
        "VirtualHost": "/",
        "User": "guest",
        "Password": "guest",
        "Queue": {
            "Name": "ExampleQueue",
            "Bindings": [
                {
                    "Exchange": "amq.topic",
                    "RoutingKey": "input"
                }
            ]
        },
        "PrefetchCount": 10
    },
    "RabbitMQPublisher": {
        "Host": "localhost",
        "VirtualHost": "/",
        "User": "guest",
        "Password": "guest",
        "PublishingTarget": {
            "Exchange": "amq.topic",
            "RoutingKey": "ouput"
        }
    }
}